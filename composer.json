{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "doctrine/dbal": "^2.12.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "irazasyed/telegram-bot-sdk": "^3.4",
        "jomweb/billplz": "^4.3",
        "laravel/framework": "^7.24",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "mailjet/laravel-mailjet": "^2.1",
        "nesbot/carbon": "^2.54",
        "php-http/guzzle7-adapter": "^1.0",
        "rap2hpoutre/fast-excel": "^3.0",
        "s-ichikawa/laravel-sendgrid-driver": "^3.0",
        "santigarcor/laratrust": "^6.1",
        "sendgrid/sendgrid": "^7.9",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "stripe/stripe-php": "^7.57"
    },
    "require-dev": {
        "bangnokia/laravel-serve-livereload": "^1.1",
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
